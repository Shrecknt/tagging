<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Upload</title>
    <link rel="shortcut icon" href="/favicon.ico">
    <link rel="stylesheet" href="/generic_styles.css">
    <style>
        body {
            text-align: center;
        }
    </style>
</head>

<body>
    <h1>Uploaded</h1>
    <noscript>JavaScript must be enabled to continue</noscript>
    <script>
        const urlParams = new URLSearchParams(window.location.search);
        const userid = urlParams.get("userid");
        const fileid = urlParams.get("fileid");
        const url = `/file/${userid}/${fileid}`;

        const elem = document.createElement("h3");
        elem.innerText = `Resource uploaded to ${url}`;
        document.body.appendChild(elem);

        window.location = `https://${window.location.host}${url}`;
    </script>
</body>

</html>